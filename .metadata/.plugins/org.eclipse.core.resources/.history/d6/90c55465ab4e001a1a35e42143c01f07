package baekjoon;

import java.util.Scanner;

public class max {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner in = new Scanner(System.in);
		int num = in.nextInt();
		
		int q = (int)num/2;
		while(q>1){
			if(num%q==0) break;
			q--;
		}
		System.out.println();
	}

}
