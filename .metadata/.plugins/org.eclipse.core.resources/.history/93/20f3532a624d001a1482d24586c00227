package baekjoon;

import java.util.Scanner;

public class dp_1463 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner in = new Scanner(System.in);
		
		int n = in.nextInt();
		int count = 0;
		
		while(n%3==0){
			n=n/3;
			count++;
		}
		while(n%2==0){
			n=n/2;
			count++;
		}
		while(n>1){
			n=n-1;
			count++;
		}
		System.out.println(count);
	}

}
