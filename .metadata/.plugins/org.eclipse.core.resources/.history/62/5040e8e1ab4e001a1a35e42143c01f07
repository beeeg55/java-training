package baekjoon;

import java.util.Scanner;

public class max {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner in = new Scanner(System.in);
		int num = in.nextInt();
		int p = (int)num/2;
		while(p>1){
			if(num%p==0){
				for(int q=2;q<=(int)p/2;q++){
					if(num%q==0) break;
					if(q==(int)p/2){
						break;
						
					}
				}
				System.out.println(p);
			}
			p--;
		}
	}

}
